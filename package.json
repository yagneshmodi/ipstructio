{
  "name": "ipstructio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"app/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"app/**/*.{ts,tsx,js,jsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx}\"",
    "check:all": "npm run lint && npm run format:check && npm run typecheck && npm run build",
    "prepare": "node .husky/install.mjs"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.50.0",
    "husky": "^8.0.0"
  }
}
